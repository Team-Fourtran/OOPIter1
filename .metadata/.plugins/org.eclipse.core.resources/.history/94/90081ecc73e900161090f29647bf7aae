import java.util.LinkedList;

public class MessageGenerator {
	private LinkedList<Mode> modes = new LinkedList<Mode>();
	private Mode currentMode;
	
	
	private void initializeModes(){
		modes.add((Mode) new UnitMode());
		modes.add((Mode) new StructureMode());
		modes.add((Mode) new RallyPointMode());
		modes.add((Mode) new EntireArmyMode());
}
}

interface Mode{
	
}

class UnitMode{
	
}

class StructureMode{
	
}

class RallyPointMode{
	
}

abstract class ArmyMode{
	
}

class EntireArmyMode extends ArmyMode{
	
}

class BattleGroupMode extends ArmyMode{
	
}

class ReinforcementMode extends ArmyMode{
	
}